{
    "name": "element-vir",
    "version": "19.0.0",
    "keywords": [
        "custom",
        "web",
        "components",
        "elements",
        "declarative",
        "lit"
    ],
    "homepage": "https://github.com/electrovir/element-vir",
    "bugs": {
        "url": "https://github.com/electrovir/element-vir/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/element-vir"
    },
    "license": "(MIT or CC0 1.0)",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "compile": "virmator compile",
        "docs": "virmator docs",
        "format": "virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "start": "npm run compile && npm install && virmator frontend",
        "test": "virmator test-web",
        "test:all": "concurrently -c auto --kill-others-on-fail --colors --names types,tests,spelling,format,docs \"npm run test:types\" \"npm run test:coverage\" \"npm run test:spelling\" \"npm run test:format\" \"npm run test:docs\"",
        "test:coverage": "virmator test-web coverage",
        "test:deps": "virmator deps check",
        "test:docs": "virmator docs check",
        "test:format": "virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^22.1.1",
        "date-vir": "^5.0.0",
        "lit": "^3.1.0",
        "lit-css-vars": "^3.0.7",
        "lit-html": "^3.1.0",
        "object-shape-tester": "^1.0.2",
        "run-time-assertions": "^0.2.1"
    },
    "devDependencies": {
        "@augment-vir/browser-testing": "^22.1.1",
        "@augment-vir/node-js": "^22.1.1",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@open-wc/testing": "^4.0.0",
        "@types/chai": "^4.3.11",
        "@types/mocha": "^10.0.6",
        "@web/dev-server-esbuild": "^1.0.1",
        "@web/test-runner": "^0.18.0",
        "@web/test-runner-commands": "^0.9.0",
        "@web/test-runner-playwright": "^0.11.0",
        "@web/test-runner-visual-regression": "^0.9.0",
        "ansi-colors": "^4.1.3",
        "concurrently": "^8.2.2",
        "cspell": "^8.2.1",
        "dependency-cruiser": "^15.5.0",
        "element-book": "^10.1.10",
        "element-vir": "file:./",
        "esbuild": "^0.19.10",
        "istanbul-smart-text-reporter": "^1.1.3",
        "markdown-code-example-inserter": "^0.3.3",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "npm-check-updates": "~16.12.3",
        "nyc": "^15.1.0",
        "prettier": "^3.1.1",
        "prettier-plugin-interpolated-html-tags": "^1.0.3",
        "prettier-plugin-jsdoc": "^1.1.1",
        "prettier-plugin-multiline-arrays": "^3.0.1",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prettier-plugin-packagejson": "^2.4.7",
        "prettier-plugin-sort-json": "^3.1.0",
        "prettier-plugin-toml": "^2.0.1",
        "ts-node": "^10.9.2",
        "type-fest": "^4.8.3",
        "typedoc": "^0.25.4",
        "typescript": "~5.2.2",
        "vira": "^2.5.5",
        "virmator": "^11.1.5",
        "vite": "^4.5.0",
        "vite-tsconfig-paths": "^4.2.2"
    },
    "overrides": {
        "lit": "^3.1.0",
        "element-vir": "*"
    }
}
